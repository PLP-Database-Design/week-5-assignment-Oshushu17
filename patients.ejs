<h2> Welcome to TeleMedAfrica</h2>
<h3> Patient's Page</h3>
<table>
    <thead>
        <tr>
            <th>Patient Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Language</th>
        </tr>
    </thead>
    <tbody>
            <% results.forEach(row => { %>
                <tr>
                    <td><%= row.patient_id %></td>
                    <td><%= row.first_name %></td>
                    <td><%= row.last_name %></td>
                    
                    <td><%= row.language %></td>
                </tr>
            <% }); %>
    </tbody>
    


</table>